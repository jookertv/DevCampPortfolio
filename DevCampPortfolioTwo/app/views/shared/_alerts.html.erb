<% if flash[:alert] %>
  <%=  js add_gritter(flash[:alert], title: "Jesse Mcalister Portfolio", sticky: false) %>
<% elsif flash[:error] %>
  <%=  js add_gritter(flash[:error], title: "Jesse Mcalister Portfolio", sticky: false) %>
<% else flash[:notice] %>
  <%=  js add_gritter(flash[:notice], title: "Jesse Mcalister Portfolio", sticky: false) %>
<% end %>